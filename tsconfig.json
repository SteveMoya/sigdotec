{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
       "@/*": [
        "./src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@src/*": [
        "src/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@data/*": [
        "src/data/*"
      ],
      "@schemas/*": [
        "src/schemas/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
      "@mocks/*": [
        "src/mocks/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@models/*": [
        "src/models/*"
      ]
    }
  }
}